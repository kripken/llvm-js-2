echo "a"
~/Dev/emscripten/emcc $1 -c -o $1.bc
echo "b"
~/Dev/nacl_sdk/pepper_29/toolchain/linux_x86_pnacl/newlib/bin/pnacl-opt -pnacl-abi-simplify-preopt $1.bc > temp1
echo "c"
~/Dev/nacl_sdk/pepper_29/toolchain/linux_x86_pnacl/newlib/bin/pnacl-opt -O2 temp1 > temp2
echo "d"
~/Dev/nacl_sdk/pepper_29/toolchain/linux_x86_pnacl/newlib/bin/pnacl-opt -pnacl-abi-simplify-postopt temp2 > $1.post.bc
rm -f temp1 temp2
echo "e"
~/Dev/int3-llvmwork/build/bin/llc -march=js -filetype=asm $1.post.bc -o $1.js

